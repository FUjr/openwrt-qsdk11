#include "qcom-ipq6018.dtsi"
#include "qcom-ipq6018-rpm-regulator.dtsi"
#include "qcom-ipq6018-cpr-regulator.dtsi"
#include "qcom-ipq6018-cp-cpu.dtsi"
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	model = "SCG5000 IPQ6010";
	compatible = "scg5000,ipq6010", "qcom,ipq6010";
	interrupt-parent = <&intc>;
	qcom,msm-id = <0x192 0x0>;

	aliases {
		ethernet0 = "/soc/dp5";
		ethernet1 = "/soc/dp4";
		ethernet2 = "/soc/dp3";
	};

	chosen {
		bootargs-append = " root=/dev/ubiblock0_1 swiotlb=1 coherent_pool=2M";
	};

	soc {
		mdio: mdio@90000 {
			pinctrl-0 = <&mdio_pins>;
			pinctrl-names = "default";
			phy-reset-gpio = <&tlmm 75 0>;
			status = "ok";

			phy0: ethernet-phy@0 {
				reg = <0>;
			};
			phy1: ethernet-phy@1 {
				reg = <1>;
			};
			phy2: ethernet-phy@2 {
				reg = <2>;
			};
			phy3: ethernet-phy@3 {
				reg = <3>;
			};
			phy4: ethernet-phy@4 {
				reg = <4>;
			};
		};

		ess-switch@3a000000 {
			switch_cpu_bmp = <0x1>;  /* cpu port bitmap */
			switch_lan_bmp = <0xe>; /* lan port bitmap */
			switch_wan_bmp = <0x10>; /* wan port bitmap */
			switch_inner_bmp = <0xc0>; /*inner port bitmap*/
			switch_mac_mode = <0x0>; /* mac mode for uniphy 0*/
			switch_mac_mode1 = <0xff>; /* mac mode for uniphy 1*/
			switch_mac_mode2 = <0xff>; /* mac mode for uniphy 2*/

			qcom,port_phyinfo {
				port@0 {
					port_id = <1>;
					phy_address = <0>;
				};
				port@1 {
					port_id = <2>;
					phy_address = <1>;
				};
				port@2 {
					port_id = <3>;
					phy_address = <2>;
				};
				port@3 {
					port_id = <4>;
					phy_address = <3>;
				};
				port@4 {
					port_id = <5>;
					phy_address = <4>;
				};
			};
		};

		dp5 {
			device_type = "network";
			compatible = "qcom,nss-dp";
			qcom,id = <5>;
			reg = <0x3a001600 0x200>;
			qcom,mactype = <0>;
			local-mac-address = [000000000000];
			qcom,link-poll = <1>;
			qcom,phy-mdio-addr = <4>;
			phy-mode = "sgmii";
		};

		dp4 {
			device_type = "network";
			compatible = "qcom,nss-dp";
			qcom,id = <4>;
			reg = <0x3a001400 0x200>;
			qcom,mactype = <0>;
			local-mac-address = [000000000000];
			qcom,link-poll = <1>;
			qcom,phy-mdio-addr = <3>;
			phy-mode = "sgmii";
		};

		dp3 {
			device_type = "network";
			compatible = "qcom,nss-dp";
			qcom,id = <3>;
			reg = <0x3a001200 0x200>;
			qcom,mactype = <0>;
			local-mac-address = [000000000000];
			qcom,link-poll = <1>;
			qcom,phy-mdio-addr = <2>;
			phy-mode = "sgmii";
		};

		nss-macsec0 {
			compatible = "qcom,nss-macsec";
			phy_addr = <0x04>;
			phy_access_mode = <0x00>;
			mdiobus = <0x52>;
		};

		gpio_keys {
			compatible = "gpio-keys";
			pinctrl-0 = <&button_pins>;
			pinctrl-names = "default";

			reset {
				label = "reset";
				linux,code = <KEY_RESTART>;
				gpios = <&tlmm 19 GPIO_ACTIVE_LOW>;
				linux,input-type = <1>;
				debounce-interval = <60>;
			};
		};

		leds {
			compatible = "gpio-leds";
			pinctrl-0 = <&leds_pins>;
			pinctrl-names = "default";

			led_wifi: led_wifi {
				label = "led_wifi";
				gpios = <&tlmm 22 GPIO_ACTIVE_HIGH>;
			};

			led_5g_r: led_5g_r {
				label = "led_5g_r";
				gpios = <&tlmm 33 GPIO_ACTIVE_HIGH>;
			};

			led_5g_g: led_5g_g {
				label = "led_5g_g";
				gpios = <&tlmm 29 GPIO_ACTIVE_HIGH>;
			};

			led_5g_b: led_5g_b {
				label = "led_5g_b";
				gpios = <&tlmm 34 GPIO_ACTIVE_HIGH>;
			};

			led_4g_r: led_4g_r {
				label = "led_4g_r";
				gpios = <&tlmm 32 GPIO_ACTIVE_HIGH>;
			};

			led_4g_g: led_4g_g {
				label = "led_4g_g";
				gpios = <&tlmm 30 GPIO_ACTIVE_HIGH>;
			};

			led_4g_b: led_4g_b {
				label = "led_4g_b";
				gpios = <&tlmm 31 GPIO_ACTIVE_HIGH>;
			};

			led_status: led_status {
				label = "led_status";
				gpios = <&tlmm 25 GPIO_ACTIVE_HIGH>;
			};

			m2_hard_poweron: m2_hard_poweron {
				label = "m2_hard_poweron";
				gpios = <&tlmm 48 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "m2_hard_poweron";
				default-state = "on";
			};

			m2_gnss_disable: m2_gnss_disable {
				label = "m2_gnss_disable";
				gpios = <&tlmm 51 GPIO_ACTIVE_LOW>;
				linux,default-trigger = "m2_gnss_disable";
				default-state = "off";
			};

			m2_fibocom_bootctrl: m2_fibocom_bootctrl {
				label = "m2_fibocom_bootctrl";
				gpios = <&tlmm 53 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "m2_fibocom_bootctrl";
				default-state = "keep";
			};

			m2_fibocom_usbboot: m2_fibocom_usbboot {
				label = "m2_fibocom_usbboot";
				gpios = <&tlmm 73 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "m2_fibocom_usbboot";
				default-state = "keep";
			};

			m2_airmode: m2_airmode {
				label = "m2_airmode";
				gpios = <&tlmm 74 GPIO_ACTIVE_LOW>;
				linux,default-trigger = "m2_airmode";
				default-state = "off";
			};

			m2_reset: m2_reset {
				label = "m2_reset";
				gpios = <&tlmm 78 GPIO_ACTIVE_LOW>;
				linux,default-trigger = "m2_reset";
				default-state = "keep";
			};

			m2_soft_poweroff_5g: m2_soft_poweroff_5g {
				label = "m2_soft_poweroff_5g";
				gpios = <&tlmm 79 GPIO_ACTIVE_LOW>;
				linux,default-trigger = "m2_soft_poweroff_5g";
				default-state = "off";
			};

			m2_soft_poweroff_4g: m2_soft_poweroff_4g {
				label = "m2_soft_poweroff_4g";
				gpios = <&tlmm 49 GPIO_ACTIVE_LOW>;
				linux,default-trigger = "m2_soft_poweroff_4g";
				default-state = "keep";
			};

			phy_reset: phy_reset {
				label = "phy_reset";
				gpios = <&tlmm 75 GPIO_ACTIVE_LOW>;
				linux,default-trigger = "phy_reset";
				default-state = "keep";
			};

			tpm_reset: tpm_reset {
				label = "tpm_reset";
				gpios = <&tlmm 62 GPIO_ACTIVE_LOW>;
				linux,default-trigger = "tpm_reset";
				default-state = "keep";
			};
		};

		pwm: pwm {
			#pwm-cells = <2>;
			pinctrl-0 = <&pwm_pinmux>;
			pinctrl-names = "default";
			used-pwm-indices = <0>, <0>, <1>, <0>;
			status = "ok";
		};

		fan: pwm-fan {
		compatible = "pwm-fan";
		cooling-min-state = <0>;
		cooling-max-state = <3>;
		#cooling-cells = <2>;
		pwms = <&pwm 2 10000 0>;
		cooling-levels = <0 102 170 230>;
		};
	};
};

&tlmm {
	i2c_1_pins: i2c_1_pins {
		mux {
			pins = "gpio42", "gpio43";
			function = "blsp2_i2c";
			drive-strength = <8>;
			bias-pull-down;
		};
	};
	pwm_pinmux: pwm_pinmux {
		mux_1 {
			pins = "gpio24";
			function = "pwm22";
			drive-strength = <8>;
		};
	};
	uart_pins: uart_pins {
		mux {
			pins = "gpio44", "gpio45";
			function = "blsp2_uart";
			drive-strength = <8>;
			bias-pull-down;
		};
	};
	spi_0_pins: spi_0_pins {
		mux {
			pins = "gpio38", "gpio39", "gpio40", "gpio41";
			function = "blsp0_spi";
			drive-strength = <8>;
			bias-pull-down;
		};
	};
	spi_4_pins: spi_4_pins {
		mux {
			pins = "gpio55", "gpio56", "gpio57", "gpio58";
			function = "blsp4_spi";
			drive-strength = <8>;
			bias-pull-down;
		};
	};

	qpic_pins: qpic_pins {
		data_0 {
			pins = "gpio15";
			function = "qpic_pad0";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_1 {
			pins = "gpio12";
			function = "qpic_pad1";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_2 {
			pins = "gpio13";
			function = "qpic_pad2";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_3 {
			pins = "gpio14";
			function = "qpic_pad3";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_4 {
			pins = "gpio5";
			function = "qpic_pad4";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_5 {
			pins = "gpio6";
			function = "qpic_pad5";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_6 {
			pins = "gpio7";
			function = "qpic_pad6";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_7 {
			pins = "gpio8";
			function = "qpic_pad7";
			drive-strength = <8>;
			bias-pull-down;
		};
		qpic_pad {
			pins = "gpio1", "gpio3", "gpio4",
			       "gpio10", "gpio11", "gpio17";
			function = "qpic_pad";
			drive-strength = <8>;
			bias-pull-down;
		};
	};

	button_pins: button_pins {
		reset_button {
			pins = "gpio19";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};

	mdio_pins: mdio_pinmux {
		mux_0 {
			pins = "gpio64";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};
		mux_1 {
			pins = "gpio65";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};
		mux_2 {
			pins = "gpio75";
			function = "gpio";
			bias-pull-up;
		};
	};

	leds_pins: leds_pins {
		led_wifi {
			pins = "gpio22";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		led_5g_r {
			pins = "gpio33";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		led_5g_g {
			pins = "gpio29";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		led_5g_b {
			pins = "gpio34";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		led_4g_r {
			pins = "gpio32";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		led_4g_g {
			pins = "gpio30";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		led_4g_b {
			pins = "gpio31";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		led_status {
			pins = "gpio25";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_hard_poweron {
			pins = "gpio48";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_gnss_disable {
			pins = "gpio51";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_fibocom_bootctrl {
			pins = "gpio53";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_fibocom_usbboot {
			pins = "gpio73";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_airmode {
			pins = "gpio74";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_reset {
			pins = "gpio78";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_soft_poweroff_5g {
			pins = "gpio79";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		m2_soft_poweroff_4g {
			pins = "gpio49";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		tpm_reset {
			pins = "gpio62";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};

	pcie_pins: pcie_pins {
		pcie0_clk {
			pins = "gpio59";
			function = "pcie0_clk";
			drive-strength = <8>;
			input-enable;
			bias-pull-up;
		};
		pcie0_rst {
			pins = "gpio60";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};
	};
};

&blsp1_uart3 {
	pinctrl-0 = <&uart_pins>;
	pinctrl-names = "default";
	status = "ok";
};

&spi_0 {
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	cs-select = <0>;
	status = "ok";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		compatible = "n25q128a11";
		linux,modalias = "m25p80", "n25q128a11";
		spi-max-frequency = <50000000>;
		use-default-sizes;
	};
};

&nand {
	pinctrl-0 = <&qpic_pins>;
	pinctrl-names = "default";
	status = "ok";
};

&nss_crypto {
	status = "ok";
};

&qpic_bam {
	status = "ok";
};

&qusb_phy_0 {
	status = "ok";
};

&ssphy_0 {
	status = "ok";
};

&usb3 {
	status = "ok";
};

&pcie_phy {
	status = "ok";
};

&pcie0 {
	status = "ok";
	pinctrl-0 = <&pcie_pins>;
	pinctrl-names = "default";
	use_delay = <0x00>;
};

&i2c_1 {
	status = "ok";
	pinctrl-0 = <&i2c_1_pins>;
	pinctrl-names = "default";

	lm75@49 {
		compatible = "nxp,lm75";
		reg = <0x49>;
		#thermal-sensor-cells = <0x01>;
	};
};